@article{dennis_ocellation_1986,
	title = {Ocellation in {Coenonympha} tullia ({Lepidoptera}: {Satyridae}) {II}. {Population} differentiation and clinal variation in the context of climatically-induced antipredator defense strategies},
	volume = {37},
	abstract = {Thirty-one populations of C. tullia from the British Isles [UK] (914 males and 540 females) have been scored for six forewing and eight hindwing spots, on all wings and surfaces, as well as for wing expanse and wing damage. Multivariate statistical techniques have been used to compare the populations simultaneously on all spot variables and population summary counts have been related to latitude, altitude and climatic gradients. Females exceed males in spotting and wing expanse but geographical patterns are similar for both sexes. Multivariate analyses of population data reveal a distinct clinal trend corresponding to the dramatic decrease in spotting north into Scotland. Thus, much of the variation in spotting can be represented by a single vector (cf. Figs 8,9). Traditional subspecies units (subsp. 'davus', 'polydama', 'scotica') can be separated as non-overlapping envelopes in the multivariate space on spotting, but inter-population variability is extensive, especially for the southernmost groups, 'polydama' and 'davus'. Only 'scotica' consistently emerges as a distinctive and compact group of populations in all analyses. Spotting in populations decreases significantly with latitude and altitude. This association is interpreted as an influence through climate, particularly through flight season sunshine levels and temperatures which are highly correlated with spotting (rs {\textgreater} 0.79 and {\textgreater} 0.65, respectively; see Fig. 10). Altogether, climate can account for 71-86\% of the interpopulation variability in spotting. It is argued that levels of spotting in C. tullia populations are mainly the outcome of opposing selection pressures for crypsis on the one hand and for deflecting attacks on the other, which respectively are determined by levels of activity and, thus ultimately, by ambient conditions. In the limited sunshine and reduced temperatures at the Scottish end of the cline, where C. tullia is less active and more vulnerable to predation, selection has been for crypsis against the vegetation substrate and thus for lowering spotting. In the sunnier, warmer conditions at the southern end of the cline, where C. tullia is more active and can effectively escape predator strikes, selection has been for large and numerous spots which act as decoys. Data on environmental conditions, behaviour, predators and wing damage concur with this hypothesis. Current phenetic relationships between populations on spotting are briefly reviewed against a background of Devensian glaciation and the development of climax vegetation in the present Flandrian interglacial.},
	number = {3},
	journal = {Entomol. Gaz.},
	author = {Dennis, R. L. H. and Porter, K. and Williams, W. R.},
	year = {1986},
	note = {bibtex: dennis\_ocellation\_1986},
	pages = {133--172}
}

