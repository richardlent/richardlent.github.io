@article{rykiel_testing_1996,
	title = {Testing ecological models: the meaning of validation},
	volume = {90},
	issn = {0304-3800},
	url = {file:///Users/rlent/test/PDF/Rykiel_1996-0792445696/Rykiel_1996.PDF},
	abstract = {The ecological literature reveals considerable confusion about the meaning of validation in the context of simulation models. The confusion arises as much from semantic and philosophical considerations as from the selection of validation procedures. Validation is not a procedure for testing scientific theory or for certifying the [`]truth' of current scientific understanding, nor is it a required activity of every modelling project. Validation means that a model is acceptable for its intended use because it meets specified performance requirements. Before validation is undertaken, (1) the purpose of the model, (2) the performance criteria, and (3) the model context must be specified. The validation process can be decomposed into several components: (1) operation, (2) theory, and (3) data. Important concepts needed to understand the model evaluation process are verification, calibration, validation, credibility, and qualification. These terms are defined in a limited technical sense applicable to the evaluation of simulation models, and not as general philosophical concepts. Different tests and standards are applied to the operational, theoretical, and data components. The operational and data components can be validated; the theoretical component cannot. The most common problem with ecological and environmental models is failure to state what the validation criteria are. Criteria must be explicitly stated because there are no universal standards for selecting what test procedures or criteria to use for validation. A test based on comparison of simulated versus observed data is generally included whenever possible. Because the objective and subjective components of validation are not mutually exclusive, disagreements over the meaning of validation can only be resolved by establishing a convention.},
	number = {3},
	journal = {Ecological Modelling},
	author = {Rykiel, Edward J.},
	year = {1996},
	note = {bibtex: rykiel\_testing\_1996},
	pages = {229--244},
	file = {Full Text (PDF):/Users/rlent/Dropbox/Zotero/storage/98CFMKBM/Rykiel_1996.PDF:application/pdf}
}

@article{hoyle_global_2005,
	title = {Global warming, human population pressure, and viability of the world's smallest butterfly},
	volume = {19},
	url = {file:///Users/rlent/test/PDF/hoyle_2005-4231899648/hoyle_2005.pdf},
	abstract = {The effects of climate change and habitat destruction and their interaction are likely to be the greatest challenge to animal and plant conservation in the twenty-first century. We used the world's smallest butterfly, the Sinai baton blue (Pseudophilotes sinaicus), as an exemplar of how global warming and human population pressures may act together to cause species extinctions. We mapped the entire global range of this butterfly and obtained extensive data on the intensity of livestock grazing. As with an increasing number of species, it is confined to a network of small habitat patches and is threatened both by indirect human-induced factors (global warming) and by the direct activities of humans (in this case, livestock grazing and collection of medicinal plants). In the absence of global warming, grazing, and plant collection, our model suggested that the butterfly will persist for at least 200 years. Above a threshold intensity of global warming, the chance of extinction accelerated rapidly, implying that there may be an annual average temperature, specific to each endangered species, above which extinction becomes very much more likely. By contrast, there was no such threshold of grazing pressure2014the chance of extinction increased steadily with increasing grazing. The impact of grazing, however, decreased with higher levels of year-to-year variation in habitat quality. The effect of global warming did not depend on the future level of grazing, suggesting that the impacts of global warming and grazing are additive. If the areas of habitat patches individually fall below certain prescribed levels, the butterfly is likely to go extinct. Two patches were very important for persistence: if either were lost the species would probably go extinct. Our results have implications for the conservation management of all species whose habitats are at risk because of the direct activities of humans and in the longer term because of climate change. Probablemente, los efectos del cambio climï¿½tico y de la destrucciï¿½n del hï¿½bitat y su interacciï¿½n son el mayor reto para la conservaciï¿½n de animales y plantas en el siglo veintiuno. Utilizamos a la mariposa mï¿½s pequeï¿½a del mundo, Pseudophilotes sinaicus, como ejemplo de cï¿½mo pueden actuar conjuntamente el calentamiento global y las presiones de la poblaciï¿½n humana para causar la extinciï¿½n de especies. Elaboramos un mapa de la distribuciï¿½n global de esta mariposa y obtuvimos datos copiosos sobre la intensidad del apacentamiento de ganado. Como sucede con muchas especies, estï¿½ confinada a una red de pequeï¿½os parches de hï¿½bitat y esta amenazada tanto por factores inducidos indirectamente por humanos (calentamiento global) como por actividades humanas directas (en este caso, apacentamiento de ganado y recolecciï¿½n de plantas medicinales). En ausencia de calentamiento global, apacentamiento y recolecta de plantas, nuestro modelo sugiriï¿½ que la mariposa persistirï¿½ por lo menos 200 aï¿½os. Por encima de una intensidad umbral de calentamiento global, la probabilidad de extinciï¿½n se acelerï¿½ rï¿½pidamente, lo que implica que puede haber una temperatura media anual, especï¿½fica para cada especie amenazada, sobre la cual la extinciï¿½n es mï¿½s probable. En contraste, no hubo tal umbral de presiï¿½n de apacentamiento2014 la probabilidad de extinciï¿½n aumenta constantemente con el incremento de apacentamiento. Sin embargo, el impacto de apacentamiento decreciï¿½ con niveles mayores de variaciï¿½n anual en la calidad del hï¿½bitat. El efecto del calentamiento global no dependiï¿½ del nivel de apacentamiento en el futuro, lo que sugiere que los impactos del calentamiento global y del apacentamiento son aditivos. Si las ï¿½reas de parches de hï¿½bitat caen individualmente bajo ciertos niveles prescritos, es probable que la mariposa se extinga. Dos parches fueron muy importantes para la persistencia: si ambos desaparecieran la especie probablemente se extinguirï¿½a. Nuestros resultados tienen implicaciones para la conservaciï¿½n de todas las especies cuyo hï ½bitat esta en riesgo debido a las actividades directas de humanos y, en el largo plazo, debido al cambio climï¿½tico.},
	number = {4},
	journal = {Conservation Biology},
	author = {Hoyle, Martin and James, Mike},
	year = {2005},
	note = {bibtex: hoyle\_global\_2005},
	pages = {1113--1124},
	file = {Full Text (PDF):/Users/rlent/Dropbox/Zotero/storage/6G8U2RWV/hoyle_2005.pdf:application/pdf}
}

