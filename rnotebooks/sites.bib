@article{munyuli_assessment_2011,
	title = {Assessment of indicator species of butterfly assemblages in coffee-banana farming system in central {Uganda}},
	issn = {1365-2028},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-2028.2011.01298.x/abstract},
	doi = {10.1111/j.1365-2028.2011.01298.x},
	abstract = {Butterflies are believed to be important pollinators because they are nectar/pollen feeders of both wild and cultivated plant species occurring in natural and managed systems. However, there exist very little knowledge about their spatio-temporal distribution and diversity in agricultural systems of Sub-Sahara Africa and in Uganda. There are no protocols to monitor the status of good pollinator species in these systems. The main aim of this study was to collect baseline information about indicator species of butterfly community in coffee–banana farming systems of central Uganda. Data were collected for 1 year (2006) using banana fruit-bait traps, transect walk-and-counts and hand-nets sampling methods. Indicator species of farmland habitats were identified using indicator value method. Species that emerged as significant (P {\textless} 0.05) suitable indicators were Catopsilia florella, Junonia sophia, Bicyclus safitza, Acraea acerata, Eurema hecabe, Ypthima albida, Zizula hylax, Acraea ventura, Eurema brigitta, Neptidopsis ophione, Junonia eonone, Zizeeria kynsna, Cupidopsis cissus, Junonia chorimene and Acrae uvui. These ubiquitous species were recommended to monitoring programmes as indicator species of butterfly communities delivering pollination services to cultivated and wild plants in farmland environments of central Uganda. Transect count was proposed as the best sampling technique to monitor butterfly species that may be involved in successful pollination activities in farmlands. There is a need to develop strategies to protect linear and nonlinear natural and semi-natural habitats to offer sufficient refugia and breeding sites to butterflies inhabiting farmlands.},
	language = {en},
	urldate = {2011-10-11},
	journal = {African Journal of Ecology},
	author = {Munyuli, Théodore},
	month = oct,
	year = {2011},
	note = {bibtex: munyuli\_assessment\_2011},
	keywords = {butterfly assemblages, Central Uganda, indicator species, Lepidoptera pollination services conservation},
	file = {Wiley Full Text PDF:/Users/rlent/Dropbox/Zotero/storage/7FUJGH3S/Munyuli - Assessment of indicator species of butterfly assem.pdf:application/pdf}
}

