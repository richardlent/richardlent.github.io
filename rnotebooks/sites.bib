	note = {bibtex: huse\_implementing\_1999},
	keywords = {adaptation, artificial neural networks, behaviour, Evolutionary Biology, genetic algorithms, habitat choice, Human Genetics, individual-based model, Plant Sciences, state dependence, stochastic dynamic programming},
	pages = {469--483},
	file = {Full Text PDF:/Users/rlent/Dropbox/Zotero/storage/3JFWNB8F/Huse et al. - 1999 - Implementing behaviour in individual-based models .pdf:application/pdf;Huse_et_al_1999.pdf:/Users/rlent/Dropbox/Zotero/storage/U3EIA97Z/Huse_et_al_1999.pdf:application/pdf;Snapshot:/Users/rlent/Dropbox/Zotero/storage/VWMNP6KP/10.html:text/html}
