@article{rodenhouse_potential_2008,
  title = {Potential Effects of Climate Change on Birds of the {{Northeast}}},
  volume = {13},
  issn = {1381-2386},
  url = {file:///Users/rlent/test/PDF/Rodenhouse_2008-1329533441/Rodenhouse_2008.pdf},
  doi = {10.1007/s11027-007-9126-1},
  abstract = {We used three approaches to assess potential effects of climate change on birds of the Northeast. First, we created distribution and abundance models for common bird species using climate, elevation, and tree species variables and modeled how bird distributions might change as habitats shift. Second, we assessed potential effects on high-elevation birds, especially Bicknell's thrush (Catharus bicknelli), that may be particularly vulnerable to climate change, by using statistical associations between climate, spruce-fir forest vegetation and bird survey data. Last, we complemented these projections with an assessment of how habitat quality of a migratory songbird, the black-throated blue warbler (Dendroica caerulescens) might be affected by climate change. Large changes in bird communities of the Northeast are likely to result from climate change, and these changes will be most dramatic under a scenario of continued high emissions. Indeed, high-elevation bird species may currently be at the threshold of critical change with as little as 1$^\circ$C warming reducing suitable habitat by more than half. Species at mid elevations are likely to experience declines in habitat quality that could affect demography. Although not all species will be affected adversely, some of the Northeast's iconic species, such as common loon and black-capped chickadee, and some of its most abundant species, including several neotropical migrants, are projected to decline significantly in abundance under all climate change scenarios. No clear mitigation strategies are apparent, as shifts in species' abundances and ranges will occur across all habitat types and for species with widely differing ecologies.},
  timestamp = {2016-03-12T15:29:37Z},
  number = {5},
  journaltitle = {Mitigation and Adaptation Strategies for Global Change},
  author = {Rodenhouse, N. and Matthews, S. and McFarland, K. and Lambert, J. and Iverson, L. and Prasad, A. and Sillett, T. and Holmes, R.},
  date = {2008},
  pages = {517--540--540},
  file = {Full Text (PDF):/Users/rlent/Dropbox/Zotero/storage/MWASZAMC/Rodenhouse_2008.pdf:application/pdf}
}

